<Application x:Class="SimpleMVVMWpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SimpleMVVMWpf"
             xmlns:Converters="clr-namespace:SimpleMVVMWpf.Converter"
             xmlns:ViewModel="clr-namespace:SimpleMVVMWpf.ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ViewModel:ViewModelLocator x:Key="ViewModelLocator" />
        
        <Converters:PopupEnumToPageConverter x:Key="PopupEnumToPageConverter"/>
    </Application.Resources>
</Application>
