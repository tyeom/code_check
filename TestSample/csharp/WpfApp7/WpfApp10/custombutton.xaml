<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="CustomTabItem" TargetType="{x:Type TabItem}">
        <Setter Property="Header" Value="ResourceDictionary"/>
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Button x:Name="gdgd" Content="ResourceDictionary Button" Command="{Binding AClickCommand, RelativeSource={RelativeSource AncestorType={x:Type Window}}}"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>